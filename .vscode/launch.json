    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Core Launch (ConsoleApp1.Web)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build ConsoleApp1.Web",
            "program": "${workspaceFolder}/ConsoleApp1.Web/bin/Debug/net10.0/ConsoleApp1.Web.dll",
            "args": [],
            "cwd": "${workspaceFolder}/ConsoleApp1.Web",
            "stopAtEntry": false,
            "serverReadyAction": {
                "action": "openExternally",
                "pattern": "\\bNow listening on:\\s+(https?://\\S+)"
            },
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development"
            }
        }
    ]
}